<plugin id="cordova-plugin-myplugin" version="1.0.0"
    xmlns="http://apache.org/cordova/ns/plugins"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <name>MyPlugin</name>

    <js-module src="www/MyPlugin.js" name="MyPlugin">
        <clobbers target="MyPlugin" />
    </js-module>

    <platform name="android">
        <config-file target="res/xml/config.xml" parent="/*">
            <feature name="MyPlugin">
                <param name="android-package" value="com.ofe.myplugin.MyPlugin" />
            </feature>
        </config-file>
        <source-file src="src/android/MyPlugin.java" target-dir="src/com/ofe/myplugin" />
    </platform>

    <platform name="ios">
		<config-file target="config.xml" parent="/*">
			<feature name="MyPlugin">
				<param name="ios-package" value="MyPlugin" />
			</feature>
		</config-file>
		<source-file src="src/ios/MyPlugin.m" />
	</platform>
</plugin>
